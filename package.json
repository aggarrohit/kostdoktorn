{
  "name": "Kostdoktorn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "detox-android-build": "detox build -c android.emu.debug",
    "detox-android-build-phone": "detox build -c android.att.debug",
    "detox-android": "detox test -c android.emu.debug",
    "detox-android-cucu": "cross-env DETOX_CONFIGURATION=android.emu.debug ./node_modules/.bin/cucumber-js --require-module @babel/register e2e/features/*/*.feature",
    "detox-android-cucu-phone": "cross-env DETOX_CONFIGURATION=android.att.debug ./node_modules/.bin/cucumber-js --require-module @babel/register e2e/features/*/*.feature"
  },
  "dependencies": {
    "@apollo/client": "^3.8.5",
    "@testing-library/react-native": "^12.3.0",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-native": "0.72.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.20.0",
    "@cucumber/cucumber": "^10.0.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "cross-env": "^7.0.3",
    "detox": "^20.13.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "uglify-js": "^3.17.4"
  },
  "engines": {
    "node": ">=16"
  }
}
